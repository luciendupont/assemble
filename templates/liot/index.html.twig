{% extends 'base.html.twig' %}

{% block title %}Libertés, Indépendants, Outre-mer et Territoires





{% endblock %}

{% block body %}


<div class="container mt-4">
<table class="table table-hover">
  <H1>Libertés, Indépendants, Outre-mer et Territoires
</H1>

    
        
    
    <thead>
      <tr>
        <th scope="col">numero</th>
        <th scope="col">nom</th>
        <th scope="col">prénom</th>
        <th scope="col">région</th>
        <th scope="col">département</th>
        <th scope="col">circonscription</th>
        <th scope="col">profession</th>
      </tr>
    </thead>
    <tbody>
{% for liot in liots %}
      <tr class="table-light">

    

        <td>{{ liot.id}}</td>
        <td>{{ liot.nomliot}}</td>
        <td>{{ liot.preliot}}</td>
        <td>{{ liot.regliot}}</td>  
        <td>{{liot.depliot}}</td>
        <td>{{ liot.cirliot}}</td>
        <td>{{liot.proliot}}</td>
     </tr>
   {% endfor %}
    </tbody>
  </table>
</div>
{% endblock %}
