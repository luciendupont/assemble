{% extends 'base.html.twig' %}

{% block title %}le  groupe Démocrate (MoDem et Indépendants)
{% endblock %}

{% block body %}


<div class="container mt-4">
<table class="table table-hover">
    <H1>le  groupe Démocrate (MoDem et Indépendants)</H1>

    
        
    
    <thead>
      <tr>
        <th scope="col">numero</th>
        <th scope="col">nom</th>
        <th scope="col">prénom</th>
        <th scope="col">région</th>
        <th scope="col">département</th>
        <th scope="col">circonscription</th>
        <th scope="col">profession</th>
      </tr>
    </thead>
    <tbody>
{% for modem in modems %}
      <tr class="table-light">

    

        <td>{{ modem.nommodem}}</td>
        <td>{{modem.prenommodem}}</td>
        <td>{{modem.regionmodem}}</td>  
        <td>{{modem.depmodem}}</td>
        <td>{{modem.circomodem}}</td>
        <td>{{modem.promodem}}</td>
     </tr>
   {% endfor %}
    </tbody>
  </table>
</div>
{% endblock %}
